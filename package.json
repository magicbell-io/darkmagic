{
  "name": "darkmagic",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "author": "Stephan Meijer",
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "clean": "manypkg exec yarn clean",
    "start": "turbo run start --parallel --no-cache",
    "start:example": "yarn --cwd example && yarn --cwd example start",
    "start:storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "start:playroom": "playroom start --config .playroom/playroom.config.cjs",
    "build": "run-s build:packages build:storybook build:playroom",
    "build:packages": "turbo run build",
    "build:example": "yarn --cwd example && yarn --cwd example build",
    "build:storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook -o dist/storybook && node .storybook/post-build.js",
    "build:playroom": "cross-env NODE_OPTIONS=--openssl-legacy-provider playroom build --config .playroom/playroom.config.cjs",
    "lint": "run-s lint:*",
    "lint:typescript": "tsc --noEmit",
    "lint:eslint": "eslint --fix .",
    "test": "vitest -c scripts/vite/vite.config.js --run --coverage",
    "test:watch": "vitest -c scripts/vite/vite.config.js",
    "test:ci": "cross-env CI=true vitest -c scripts/vite/vite.config.js --passWithNoTests",
    "postinstall": "husky install && node scripts/sync-paths.js && manypkg check",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:release": "yarn build && yarn prettier --write '{packages,example}/*/{package.json,CHANGELOG.md}' '.changeset/*.json' && changeset publish",
    "console": "ts-node"
  },
  "lint-staged": {
    "*.{md,json,yml,yaml}": [
      "prettier -w"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=v12.22.12"
  },
  "dependencies": {
    "@babel/core": "^7.22.11",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@manypkg/cli": "^0.21.0",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vanilla-extract/babel-plugin-debug-ids": "^1.0.3",
    "@vitest/coverage-c8": "^0.23.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "playroom": "^0.31.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "replace-in-files": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "size-limit": "^8.2.4",
    "tiny-glob": "^0.2.9",
    "tslib": "^2.5.3",
    "tsx": "^3.12.7",
    "turbo": "^1.10.4",
    "typescript": "^5.2.2",
    "vite": "^3.1.0",
    "vite-plugin-banner": "^0.5.0",
    "vitest": "^0.23.1",
    "vitest-github-actions-reporter": "^0.8.3"
  }
}
